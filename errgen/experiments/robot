x = 0;
y = 0;
t = 0;
two = 2;
while(true) {
 c = 6;
 c = c - tp;
 c = c + yp;
 c = c - y;
 c = c + t;
 assume(c >= 0);
 c = 3;
 c = c + y;
 tt = two * t;
 ttp = two * tp;
 c = c + tt;
 c = c - ttp;
 assume(c <= 0);
 c = x - xp;
 c = c - tt;
 c = c + ttp;
 assume(c >= 0);
 t = tp;
 y = yp;
 x = xp
}
